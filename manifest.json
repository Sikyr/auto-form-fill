{
	"manifest_version": 3,
	"name": "AutoFill",
	"description": "Auto fill forms",
	"version": "1.0.0",
	
	"icons": {
			"128": "icon128.png"
		},
		
	"permissions": ["storage", "nativeMessaging", "tabs"],
		
	"content_scripts":[
		{
			"matches":[
				"https://mojezasielky.posta.sk/*",
				"http://mojezasielky.posta.sk/*"
			],
			"js": ["content/slovFill.js"]
		},
		{
			"matches":[
				"https://e-nadawca.poczta-polska.pl/*",
				"http://e-nadawca.poczta-polska.pl/*"
			],
			"js": ["content/polFill.js"]
		}
	],
	
	"options_page": "settings-page/settings.html",

	"action": {
        "default_title": "AutoFill",
        "default_icon": "icon128.png",
        "default_popup": "popup/popup.html"
    }
}